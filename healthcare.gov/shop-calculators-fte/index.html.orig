                                                <%= lang.t('worksheet_weekly') %>
                                            </label>
                                            <label class="btn btn-primary radio-label" for="entry-method-1" id="entry-method-1-lbl">
                                                <input type="radio" id="entry-method-1" name="entry-method" value="1" aria-label="Monthly" data-toggle="button">
                                                <%= lang.t('worksheet_monthly') %>
                                            </label>
                                            <label class="btn btn-primary radio-label" for="entry-method-2" id="entry-method-2-lbl">
                                                <input type="radio" id="entry-method-2" name="entry-method" value="2" aria-label="Yearly" data-toggle="button">
                                                <%= lang.t('worksheet_yearly') %>
                                            </label>
                                        </div>
                                    </div>
